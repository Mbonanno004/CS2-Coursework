String infixToPrefix(const std::ifstream &in) {
    if (!in.is_open()) {
        std::cerr << "Error: File is not open" << std::endl;
        return;
    }

    std::vector<String> expressions;
    char buffer[1024];

    while () {
        if (in >> buffer) {
            String expression(buffer);
        }
    }
}