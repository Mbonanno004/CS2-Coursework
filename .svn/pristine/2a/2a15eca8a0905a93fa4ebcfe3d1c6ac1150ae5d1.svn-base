//  String Test Program
//
//  Tests: Default Constructor, uses ==
//
//  
#include <iostream>
#include <cassert>
#include "string.hpp"

int main () {
    {
        // Setup fixture
        char test = '\0';

        // Test 
        String tester(test);
        std::cout << "string = " << tester << std::endl;


        // Verify
        assert(tester == '\0');
    }

    {
        std::cout << "Finished testing default ctor" << std::endl;
    }
}